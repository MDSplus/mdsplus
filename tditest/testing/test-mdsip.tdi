fun check_user(in _user, in _host, in _ip) {\
	_match = [strdup(_user//"@"//_host),strdup(_user//"@"//_ip)];\
	_res = MdsIpShr->CheckClient(ref(_user),val(2),ref(_match));\
	free(_match[0]);free(_match[1]);return(_res);\
};\
