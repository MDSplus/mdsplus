#!/bin/bash

cd "$(dirname "${BASH_SOURCE[0]}")"

PYTHON=${PYTHON:-python3}
GPERF=${GPERF:-gperf}

OPCODES=tdishr/opcodes.csv

$PYTHON tdishr/gen-TdiHash.py \
	--opcodes=$OPCODES \
	--gperf=$GPERF \
	--output=tdishr/TdiHash.c

$PYTHON python/MDSplus/gen-compound.py \
	--opcodes=$OPCODES \
	--input=python/MDSplus/compound.py.in \
	--output=python/MDSplus/compound.py

$PYTHON deploy/gen-yacc-lex.py \
	