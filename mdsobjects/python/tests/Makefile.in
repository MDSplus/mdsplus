.PHONY: clean-local-tests
clean-local-tests:
	-$(RM) -rf $(TEST_BUILD_TAP)
	-$(RM) -rf $(TEST_SUITE_TAP)
	-$(RM) -rf $(TEST_LOGS:.log=.tap)
	-$(RM) -rf $(TEST_SUITE_XML)
	-$(RM) -rf $(TEST_LOGS:.log=.xml)

