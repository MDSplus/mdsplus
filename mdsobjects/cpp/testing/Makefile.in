	@ \
	if [ -n "$(check_PROGRAMS)" ]; then \
	 echo "//// Rebuilding tests ////"; \
	 rm -f *.$(OBJEXT); \
	 rm -f $(check_PROGRAMS); \
	 if [ -n "$${VALGRIND_BUILD}" ]; then \
	  $(MAKE) -k $(AM_MAKEFLAGS) $(BUILD_FLAGS) all ||:; \
	 else \
	  $(MAKE) -k $(AM_MAKEFLAGS) $(BUILD_FLAGS) all \
	  CFLAGS="${CFLAGS} -D_TESTING" CPPFLAGS="${CPPFLAGS} -D_TESTING" ||:; \
	 fi; \
	fi

