@MINGW_TRUE@ IMPLIB=@MAKELIBDIR@TdiShr.dll.a

LIBS=-L@MAKESHLIBDIR@ @LIBS@ -lTreeShr -lMdsShr @LIBM@ $(THREAD) @LIBDL@ $(LIBSOCKET)
