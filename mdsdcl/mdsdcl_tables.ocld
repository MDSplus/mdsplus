	Parameter P1,VALUE(REQUIRED)
	Parameter P2,LABEL=MACRO,VALUE
        Qualifier full
Define syntax SHOW_TIMER
	Routine mdsdcl_show_timer
	Parameter P1,VALUE(REQUIRED)
!Define syntax SHOW_VM
!	Routine MDSDCL_SHOW_VM
!	Parameter P1,VALUE(REQUIRED)
