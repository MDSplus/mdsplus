	Parameter P1,LABEL=SYMBOL,PROMPT="Symbol name",VALUE(REQUIRED)						       !4
	Parameter P2,LABEL=VALUE,PROMPT="Value",VALUE(REQUIRED,TYPE=$REST_OF_LINE)				       !4
